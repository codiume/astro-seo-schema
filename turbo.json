{
  "$schema": "https://turbo.build/schema.json",

  "tasks": {
    "test": {
      "outputs": [],
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^test",
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^typecheck"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}

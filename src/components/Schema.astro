---
import { JsonLd } from "../jsonld";
import type { Graph, Thing, WithContext } from "schema-dts";

export type Props = {
    item: Graph | WithContext<Thing>;
    space?: string | number;
};

const { item, space } = Astro.props;
---

<script type="application/ld+json" set:html={JsonLd(item, space)}></script>
